{
    "manifest_version": 2,
    "minimum_chrome_version": "24.0.1307.0",
    "name": "Codebox For Your Desktop",
    "description": "Cloud IDE as a Service.",
    "offline_enabled": true,
    "permissions": [
        "unlimitedStorage", "webview"
    ],
    "version": "2",
    "app": {
        "background": {
            "scripts": ["main.js"]
        }
    },
    "icons": {
        "128": "icons/128.png",
        "32": "icons/32.png"
    }
}