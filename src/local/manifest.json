{
    "manifest_version": 2,
    "name": "Codebox",
    "description": "Cloud IDE as a Service.",
    "offline_enabled": true,
    "permissions": [
        "unlimitedStorage"
    ],
    "version": "2",
    "app": {
        "urls": [
            "*://localhost"
        ],
        "launch": {
            "web_url": "http://localhost:8000",
            "container": "panel"
        }
    },
    "icons": {
        "128": "icons/128.png",
        "32": "icons/32.png"
    }
}