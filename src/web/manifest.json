{
    "manifest_version": 2,
    "name": "Codebox",
    "description": "Powerful, Collaborative Online/Offline cloud IDE",
    "offline_enabled": true,
    "permissions": [
        "unlimitedStorage"
    ],
    "version": "2.4",
    "app": {
        "urls": [
            "*://www.codebox.io/"
        ],
        "launch": {
            "web_url": "https://www.codebox.io/"
        }
    },
    "icons": {
        "128": "icons/128.png",
        "32": "icons/32.png"
    }
}