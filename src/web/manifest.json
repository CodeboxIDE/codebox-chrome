{
    "manifest_version": 2,
    "name": "Codebox",
    "description": "Powerful, Collaborative Online/Offline web IDE",
    "offline_enabled": true,
    "permissions": [
        "unlimitedStorage"
    ],
    "version": "2.2",
    "app": {
        "urls": [
            "*://www.codebox.io/"
        ],
        "launch": {
            "web_url": "https://www.codebox.io/?standalone=true",
            "container": "panel"
        }
    },
    "icons": {
        "128": "icons/128.png",
        "32": "icons/32.png"
    }
}